{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "NIXPACKS",
      "buildCommand": "pip install -r requirements.txt",
      "nixpacksPlan": {
        "providers": ["python"],
        "phases": {
          "install": {
            "cmds": ["pip install -r requirements.txt"]
          }
        }
      }
    },
    "deploy": {
      "startCommand": "python bot.py"
    }
  }
  